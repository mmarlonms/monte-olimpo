{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    }
  },
  "ElasticConfiguration": {
    "UseAuthentication": false,
    "UrlHost": "http://localhost:9200",
    "IndexName": "monteolimpo-api"
  },
  "LogConfiguration": {
    "EnableLog": true,
    "EnableLogConsole": true,
    "EnableLogElasticSearch": true
  }
}
